{
    "name": "deviffy/laravel-module-builder",
    "description": "Laravel Module Builder",
    "keywords": ["framework", "laravel", "module builder", "crud", "package"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Catalin Dumitrescu",
            "email": "onecdumitrescu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4"
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    }
}
